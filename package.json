    "@nuxt/image": "1.1.0",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/device": "^3.1.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/google-fonts": "^3.1.3",
    "@nuxtjs/i18n": "8.0.0",
    "@nuxtjs/tailwindcss": "^6.10.3",
    "@pinia/nuxt": "^0.5.1",
    "@sentry/node": "^7.98.0",
    "@sentry/profiling-node": "^1.3.5",
