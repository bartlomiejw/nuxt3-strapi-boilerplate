    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "nuxt": "^3.9.1",
    "nuxt-graphql-client": "0.2.28",
    "nuxt-icon": "^0.6.8",
    "nuxt-primevue": "^0.2.2",
    "pinia": "^2.1.7",
    "postcss-import": "^16.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
